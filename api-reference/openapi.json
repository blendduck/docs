{
  "openapi": "3.0.0",
  "info": {
    "version": "1.0.0",
    "title": "BlendDuck REST API",
    "description": "BlendDuck REST API for dynamic video creation"
  },
  "servers": [
    {
      "url": "https://blendduck.com/api/v1"
    }
  ],
  "components": {
    "schemas": {
      "ProjectMeta": {
        "type": "object",
        "properties": {
          "version": {
            "type": "string"
          }
        },
        "required": [
          "version"
        ]
      },
      "PureColor": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": [
              "color"
            ]
          },
          "color": {
            "type": "string"
          }
        },
        "required": [
          "type",
          "color"
        ]
      },
      "GradientColor": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": [
              "gradient"
            ]
          },
          "gradient": {
            "type": "object",
            "properties": {
              "gradient": {
                "type": "string"
              },
              "angle": {
                "type": "number"
              }
            },
            "required": [
              "gradient",
              "angle"
            ]
          }
        },
        "required": [
          "type",
          "gradient"
        ]
      },
      "Background": {
        "type": "object",
        "properties": {
          "color": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/PureColor"
              },
              {
                "$ref": "#/components/schemas/GradientColor"
              }
            ]
          },
          "media": {
            "type": "object",
            "nullable": true,
            "properties": {
              "type": {
                "type": "string"
              },
              "name": {
                "type": "string",
                "nullable": true
              },
              "url": {
                "type": "string",
                "format": "uri"
              }
            },
            "required": [
              "type",
              "url"
            ]
          }
        },
        "required": [
          "color"
        ]
      },
      "ProjectTheme": {
        "type": "object",
        "properties": {
          "background": {
            "$ref": "#/components/schemas/Background"
          },
          "fontFamily": {
            "type": "string"
          },
          "fontColor": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/PureColor"
              },
              {
                "$ref": "#/components/schemas/GradientColor"
              }
            ]
          }
        },
        "required": [
          "background",
          "fontFamily",
          "fontColor"
        ]
      },
      "Animation": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "enum": [
              "move",
              "scale",
              "rotate",
              "opacity",
              "color",
              "appear",
              "fadeIn",
              "slideIn",
              "maskIn",
              "scaleIn",
              "disappear",
              "fadeOut",
              "slideOut",
              "maskOut",
              "scaleOut"
            ]
          },
          "startTime": {
            "type": "number",
            "minimum": 0
          },
          "endTime": {
            "type": "number",
            "minimum": 0
          },
          "easing": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "type",
          "startTime",
          "endTime",
          "easing"
        ]
      },
      "BaseElement": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "type": {
            "type": "string"
          },
          "x": {
            "type": "number"
          },
          "y": {
            "type": "number"
          },
          "width": {
            "type": "number",
            "minimum": 0
          },
          "height": {
            "type": "number",
            "minimum": 0
          },
          "rotation": {
            "type": "number"
          },
          "scale": {
            "type": "number",
            "minimum": 0
          },
          "opacity": {
            "type": "number",
            "minimum": 0,
            "maximum": 1
          },
          "animations": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Animation"
            }
          }
        },
        "required": [
          "id",
          "type",
          "x",
          "y",
          "width",
          "height",
          "rotation",
          "scale",
          "opacity",
          "animations"
        ]
      },
      "Text": {
        "allOf": [
          {
            "$ref": "#/components/schemas/BaseElement"
          },
          {
            "type": "object",
            "properties": {
              "type": {
                "type": "string",
                "enum": [
                  "text"
                ]
              },
              "text": {
                "type": "string"
              },
              "fontWeight": {
                "type": "number"
              },
              "fontSize": {
                "type": "number"
              },
              "bold": {
                "type": "boolean"
              },
              "italic": {
                "type": "boolean"
              },
              "underline": {
                "type": "boolean"
              },
              "strikethrough": {
                "type": "boolean"
              },
              "textStyle": {
                "type": "string",
                "enum": [
                  "none",
                  "shadow",
                  "stack",
                  "stage",
                  "neon",
                  "glitch"
                ]
              },
              "textAlign": {
                "type": "string",
                "enum": [
                  "left",
                  "center",
                  "right"
                ]
              },
              "autoSize": {
                "type": "boolean"
              }
            },
            "required": [
              "text",
              "fontWeight",
              "fontSize",
              "bold",
              "italic",
              "underline",
              "strikethrough",
              "textStyle",
              "textAlign",
              "autoSize"
            ]
          }
        ]
      },
      "Image": {
        "allOf": [
          {
            "$ref": "#/components/schemas/BaseElement"
          },
          {
            "type": "object",
            "properties": {
              "type": {
                "type": "string",
                "enum": [
                  "image"
                ]
              },
              "radius": {
                "type": "number"
              },
              "url": {
                "type": "string",
                "format": "uri"
              }
            },
            "required": [
              "radius",
              "url"
            ]
          }
        ]
      },
      "Video": {
        "allOf": [
          {
            "$ref": "#/components/schemas/BaseElement"
          },
          {
            "type": "object",
            "properties": {
              "type": {
                "type": "string",
                "enum": [
                  "video"
                ]
              },
              "radius": {
                "type": "number"
              },
              "url": {
                "type": "string",
                "format": "uri"
              },
              "volume": {
                "type": "number",
                "minimum": 0,
                "maximum": 1
              },
              "loop": {
                "type": "boolean"
              }
            },
            "required": [
              "radius",
              "url",
              "volume",
              "loop"
            ]
          }
        ]
      },
      "Emoji": {
        "allOf": [
          {
            "$ref": "#/components/schemas/BaseElement"
          },
          {
            "type": "object",
            "properties": {
              "type": {
                "type": "string",
                "enum": [
                  "emoji"
                ]
              },
              "radius": {
                "type": "number"
              },
              "url": {
                "type": "string",
                "format": "uri"
              }
            },
            "required": [
              "radius",
              "url"
            ]
          }
        ]
      },
      "Shape": {
        "allOf": [
          {
            "$ref": "#/components/schemas/BaseElement"
          },
          {
            "type": "object",
            "properties": {
              "type": {
                "type": "string",
                "enum": [
                  "shape"
                ]
              },
              "shapeId": {
                "type": "number"
              },
              "fill": {
                "anyOf": [
                  {
                    "$ref": "#/components/schemas/PureColor"
                  },
                  {
                    "$ref": "#/components/schemas/GradientColor"
                  },
                  {
                    "nullable": true
                  }
                ]
              }
            },
            "required": [
              "shapeId"
            ]
          }
        ]
      },
      "Element": {
        "allOf": [
          {
            "$ref": "#/components/schemas/BaseElement"
          },
          {
            "type": "object",
            "properties": {
              "type": {
                "type": "string",
                "enum": [
                  "addon"
                ]
              },
              "scope": {
                "type": "string"
              },
              "widgetId": {
                "type": "string"
              }
            },
            "required": [
              "scope",
              "widgetId"
            ]
          }
        ]
      },
      "Group": {
        "allOf": [
          {
            "$ref": "#/components/schemas/BaseElement"
          },
          {
            "type": "object",
            "properties": {
              "type": {
                "type": "string",
                "enum": [
                  "group"
                ]
              },
              "children": {
                "type": "array",
                "items": {
                  "nullable": true
                }
              }
            },
            "required": [
              "children"
            ]
          }
        ]
      },
      "AudioPlay": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "enum": [
              "audioPlay"
            ]
          },
          "url": {
            "type": "string",
            "format": "uri"
          },
          "name": {
            "type": "string"
          },
          "offset": {
            "type": "number",
            "minimum": 0
          },
          "startTime": {
            "type": "number",
            "minimum": 0
          },
          "endTime": {
            "type": "number",
            "minimum": 0
          },
          "volume": {
            "type": "number",
            "minimum": 0,
            "maximum": 1
          },
          "fade": {
            "type": "object",
            "nullable": true,
            "properties": {
              "in": {
                "type": "number",
                "default": 0
              },
              "out": {
                "type": "number",
                "default": 0
              }
            }
          }
        },
        "required": [
          "id",
          "type",
          "url",
          "name",
          "offset",
          "startTime",
          "endTime",
          "volume"
        ]
      },
      "ProjectClip": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "duration": {
            "type": "number",
            "minimum": 0.01
          },
          "durationLock": {
            "type": "boolean"
          },
          "transition": {
            "type": "string",
            "enum": [
              "none",
              "fade",
              "wipe",
              "flip",
              "slide",
              "clock"
            ]
          },
          "elements": {
            "type": "array",
            "items": {
              "oneOf": [
                {
                  "$ref": "#/components/schemas/Text"
                },
                {
                  "$ref": "#/components/schemas/Image"
                },
                {
                  "$ref": "#/components/schemas/Video"
                },
                {
                  "$ref": "#/components/schemas/Emoji"
                },
                {
                  "$ref": "#/components/schemas/Shape"
                },
                {
                  "$ref": "#/components/schemas/Element"
                },
                {
                  "$ref": "#/components/schemas/Group"
                }
              ],
              "discriminator": {
                "propertyName": "type",
                "mapping": {
                  "text": "#/components/schemas/Text",
                  "image": "#/components/schemas/Image",
                  "video": "#/components/schemas/Video",
                  "emoji": "#/components/schemas/Emoji",
                  "shape": "#/components/schemas/Shape",
                  "addon": "#/components/schemas/Element",
                  "group": "#/components/schemas/Group"
                }
              }
            }
          },
          "audios": {
            "type": "array",
            "nullable": true,
            "items": {
              "$ref": "#/components/schemas/AudioPlay"
            }
          },
          "background": {
            "allOf": [
              {
                "$ref": "#/components/schemas/Background"
              },
              {
                "nullable": true
              }
            ]
          }
        },
        "required": [
          "id",
          "duration",
          "durationLock",
          "transition",
          "elements"
        ]
      },
      "ProjectResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "title": {
            "type": "string"
          },
          "ratio": {
            "type": "string"
          },
          "share": {
            "type": "boolean"
          },
          "teamId": {
            "type": "string"
          },
          "userId": {
            "type": "string"
          },
          "meta": {
            "$ref": "#/components/schemas/ProjectMeta"
          },
          "theme": {
            "$ref": "#/components/schemas/ProjectTheme"
          },
          "clips": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ProjectClip"
            }
          },
          "createdAt": {
            "type": "string"
          },
          "updatedAt": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "title",
          "ratio",
          "share",
          "teamId",
          "userId",
          "meta",
          "theme",
          "clips",
          "createdAt",
          "updatedAt"
        ]
      },
      "Project json": {
        "type": "string"
      },
      "ExportResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          }
        },
        "required": [
          "id"
        ]
      },
      "ExportOptions": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "fps": {
            "type": "string",
            "enum": [
              "30",
              "60"
            ],
            "default": "30",
            "example": "30"
          },
          "resolution": {
            "type": "string",
            "enum": [
              "720p",
              "1080p",
              "4k"
            ],
            "default": "720p",
            "example": "720p"
          },
          "quality": {
            "type": "string",
            "enum": [
              "low",
              "medium",
              "high"
            ],
            "example": "medium"
          },
          "isPublic": {
            "type": "boolean",
            "default": false,
            "description": "video visibility",
            "example": false
          }
        },
        "required": [
          "id",
          "quality"
        ]
      }
    },
    "parameters": {}
  },
  "paths": {
