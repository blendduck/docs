          {
            "schema": {
              "type": "number",
              "example": 20
            },
            "required": true,
            "name": "limit",
            "in": "path"
          }
        ],
        "responses": {
          "200": {
            "description": "Project List Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "projects": {
                      "type": "array",
                      "items": {
                        "allOf": [
                          {
                            "$ref": "#/components/schemas/ProjectResponse"
                          },
                          {
                            "properties": {
                              "id": {
                                "type": "string"
                              },
                              "title": {
                                "type": "string"
                              },
                              "ratio": {
                                "type": "string"
                              },
                              "share": {
                                "type": "boolean"
                              },
                              "teamId": {
                                "type": "string"
                              },
                              "userId": {
                                "type": "string"
                              },
                              "meta": {
                                "$ref": "#/components/schemas/ProjectMeta"
                              },
                              "theme": {
                                "$ref": "#/components/schemas/ProjectTheme"
                              },
                              "clips": {
                                "type": "array",
                                "items": {
                                  "allOf": [
                                    {
                                      "$ref": "#/components/schemas/ProjectClip"
                                    },
                                    {
                                      "properties": {
                                        "id": {
                                          "type": "string"
                                        },
                                        "duration": {
                                          "type": "number",
                                          "minimum": 0.01
                                        },
                                        "durationLock": {
                                          "type": "boolean"
                                        },
                                        "transition": {
                                          "type": "string",
                                          "enum": [
                                            "none",
                                            "fade",
                                            "wipe",
                                            "flip",
                                            "slide",
                                            "clock"
                                          ]
                                        },
                                        "elements": {
                                          "type": "array",
                                          "items": {
                                            "allOf": [
                                              {
                                                "$ref": "#/components/schemas/Element"
                                              },
                                              {
                                                "oneOf": [
                                                  {
                                                    "$ref": "#/components/schemas/Text"
                                                  },
                                                  {
                                                    "$ref": "#/components/schemas/Image"
                                                  },
                                                  {
                                                    "$ref": "#/components/schemas/Video"
                                                  },
                                                  {
                                                    "$ref": "#/components/schemas/Emoji"
                                                  },
                                                  {
                                                    "$ref": "#/components/schemas/Shape"
                                                  },
                                                  {
                                                    "$ref": "#/components/schemas/Element"
                                                  },
                                                  {
                                                    "$ref": "#/components/schemas/Group"
                                                  }
                                                ],
                                                "discriminator": {
                                                  "propertyName": "type",
                                                  "mapping": {
                                                    "text": "#/components/schemas/Text",
                                                    "image": "#/components/schemas/Image",
                                                    "video": "#/components/schemas/Video",
                                                    "emoji": "#/components/schemas/Emoji",
                                                    "shape": "#/components/schemas/Shape",
                                                    "addon": "#/components/schemas/Element",
                                                    "group": "#/components/schemas/Group"
                                                  }
                                                }
                                              }
                                            ]
                                          }
                                        },
                                        "audios": {
                                          "type": "array",
                                          "nullable": true,
                                          "items": {
                                            "$ref": "#/components/schemas/AudioPlay"
                                          }
                                        },
                                        "background": {
                                          "allOf": [
                                            {
                                              "$ref": "#/components/schemas/Background"
                                            },
                                            {
                                              "nullable": true
                                            }
                                          ]
                                        }
                                      }
                                    }
                                  ]
                                }
                              },
                              "createdAt": {
                                "type": "string"
                              },
                              "updatedAt": {
                                "type": "string"
                              }
                            }
                          }
                        ]
                      }
                    },
                    "pagination": {
                      "type": "object",
                      "properties": {
                        "page": {
                          "type": "number"
                        },
                        "limit": {
                          "type": "number"
                        },
                        "total": {
                          "type": "number"
                        }
                      },
                      "required": [
                        "page",
                        "limit",
                        "total"
                      ]
                    }
                  },
                  "required": [
                    "projects",
                    "pagination"
                  ]
                }
              }
            }
          }
        }
      }
